{
    "builds": [
        {
            "src": "api/index.go",
            "use": "@vercel/go"
        }
    ],
    "routes": [
        {
            "src": "/api/graphql",
            "dest": "/api/index.go"
        },
        {
            "src": "/api/query",
            "dest": "/api/index.go"
        },
        {
            "src": "/api/playground",
            "dest": "/api/index.go"
        },
        {
            "src": "/api",
            "dest": "/api/index.go"
        },
        {
            "src": "/(.*)",
            "dest": "/api/index.go"
        }
    ]
}